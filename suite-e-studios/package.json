{
  "name": "suite-e-studios",
  "main": "expo-router/entry",
  "version": "1.0.8",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "rebuild": "rm -rf node_modules && rm -rf package-lock.json && npm cache clean --force && npm install --legacy-peer-deps",
    "web": "expo start --web",
    "web:build": "npx expo export --platform web",
    "web:build:static": "EXPO_WEB_OUTPUT=static expo export --platform web",
    "format": "prettier --write \"{app,utils,hooks,constants,types}/**/*.{js,jsx,ts,tsx}\"",
    "expo:lint": "expo lint",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "build": "expo export --platform web",
    "clean": "rm -rf node_modules dist web-build && npm cache clean --force",
    "audit": "npm audit",
    "firebase:deploy": "firebase deploy",
    "docker:build": "docker build -t suite-e-studios .",
    "docker:run": "docker run -p 3000:3000 suite-e-studios",
    "deploy": "npm run web:build && npm run firebase:deploy",
    "db:seed": "node scripts/seed-firestore.js",
    "ci": "npm run lint && npm run typecheck && npm test && npm run web:build"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@gorhom/portal": "^1.0.14",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/drawer": "^7.5.0",
    "@react-navigation/elements": "^2.5.0",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.19",
    "@sentry/react-native": "^6.15.1",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "expo": "~53.0.12",
    "expo-blur": "~14.1.5",
    "expo-clipboard": "^7.1.4",
    "expo-constants": "~17.1.6",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.3.0",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.1.0",
    "expo-splash-screen": "^0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.9",
    "expo-web-browser": "~14.2.0",
    "firebase": "^11.9.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.80.0",
    "react-native-gesture-handler": "~2.26.0",
    "react-native-reanimated": "~3.18.0",
    "react-native-safe-area-context": "^5.5.0",
    "react-native-screens": "~4.11.1",
    "react-native-toast-message": "^2.3.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.15.0",
    "react-query": "^3.39.3",
    "react-test-renderer": "^19.1.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.8",
    "@types/react-native": "^0.73.0",
    "@types/styled-components-react-native": "^5.2.5",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.29.0",
    "eslint-config-expo": "~9.2.0",
    "firebase-admin": "^13.4.0",
    "firebase-tools": "^14.8.0",
    "jest": "^30.0.2",
    "jest-environment-jsdom": "^30.0.2",
    "prettier": "^3.6.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1"
  },
  "private": true
}
